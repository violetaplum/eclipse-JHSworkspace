package sortTest;

import java.util.Arrays;

public class SortTest 
{

	public static void main(String[] args) 
	{
		// TODO Auto-generated method stub
		int[] bae = {10,90,100,60,70};
		Arrays.sort(bae); 
		
		//알아서 자동으로 정렬해준다.
		for(int i=0;i<bae.length;i++)
		{
			System.out.println(bae[i]);
	
		}
	}
}
