		int[][] tByt = new int[3][3];
		tByt[0][0] = 1;
		tByt[1][0] = 2;
		tByt[1][1] = 3;
		tByt[2][0] = 4;
		tByt[2][1] = 5;
		tByt[2][2] = 6;
		
		for(i=0;i<tByt.length;i++)
		{
			if(i==j)
			{
				if(i!=0)
				{
					continue;
				}
				
			}
			for(j=0;j<tByt[i].length;j++)
			{
				
				System.out.print(tByt[i][j]+"\t");
				
			}
			
			System.out.println();
		}
		
	}

}
