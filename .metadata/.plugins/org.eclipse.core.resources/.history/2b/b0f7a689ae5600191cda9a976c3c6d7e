package BinarySearch;
import java.util.*;

public class BinarySearch {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int[] bae = {500,102,207,15,300};
		Arrays.sort(bae);
		for(int i=0;i<bae.length;i++)
		{
			System.out.println(bae[i]+"\t");
		}
		System.out.println();
		
		
	}

}
